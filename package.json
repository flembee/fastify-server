{
  "name": "fastify-server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "seed": "node ./seed",
    "serve": "node index.js",
    "start": "cross-env NODE_PATH=. NODE_ENV=development node index.js",
    "dev": "env-cmd -f ./.env nodemon --exec npm start",
    "lint": "eslint \"**/*.js\"",
    "format": "prettier --write \"**/*.js\"",
    "test:unit": "tap test/*.test.js --no-coverage-report --jobs=1",
    "add:module": "node bin/addModule.js",
    "precommit": "lint-staged && npm run test:unit"
  },
  "author": "Flembee",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^6.5.4",
    "@hapi/boom": "^10.0.0",
    "bcryptjs": "^2.4.3",
    "change-case": "^4.1.2",
    "cross-env": "^7.0.3",
    "dayjs": "^1.11.6",
    "deep-parse-json": "^1.0.2",
    "fastify": "^4.9.2",
    "fastify-boom": "^1.0.0",
    "fastify-cli": "^5.5.0",
    "fastify-cors": "^6.1.0",
    "fastify-helmet": "^7.1.0",
    "fastify-jwt": "^4.2.0",
    "fastify-multer": "^2.0.3",
    "fastify-plugin": "^4.3.0",
    "fastify-swagger": "^5.2.0",
    "http-status": "^1.5.3",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.0",
    "mongoose-paginate-v2": "^1.7.1",
    "pdf2json": "^3.0.0",
    "pino-pretty": "^9.1.1",
    "ramda": "^0.28.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "chai": "^4.3.6",
    "colors": "^1.4.0",
    "del": "^7.0.0",
    "depcheck": "1.4.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.26.0",
    "jest": "^29.2.2",
    "morgan": "^1.10.0",
    "prettier": "^2.7.1",
    "sinon": "^14.0.1",
    "supertest": "^6.3.1",
    "tap": "^16.3.0"
  },
  "optionalDependencies": {
    "lint-staged": "^13.0.3",
    "nodemon": "^2.0.20"
  }
}
